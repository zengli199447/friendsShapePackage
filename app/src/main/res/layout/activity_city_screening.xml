<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">

    <include layout="@layout/layout_title_bar" />

    <me.yokeyword.indexablerv.IndexableLayout
        android:id="@+id/city_screening"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="@dimen/dp15"
        app:indexBar_layout_width="40dp"
        app:indexBar_selectedTextColor="@color/black_overlay"
        app:indexBar_textColor="@color/gray_light_text"
        app:indexBar_textSize="@dimen/dp14"
        app:indexBar_textSpace="5dp">

    </me.yokeyword.indexablerv.IndexableLayout>


</LinearLayout>